<div>

  <h1>Login</h1>

  <form id="login-form" action='/login' method="POST">
    <label>Username</label>
      <input type="text" name="username">
    <label>Password</label>
      <input type="password" name="password">
      <input id="login-submit" type="submit">
  </form>

  <%= @error if @error %>
</div>
